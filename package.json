{
  "name": "trans-later1",
  "version": "1.0.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.konan.translater1",
    "productName": "trans-later1",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "modules/**/*",
      "renderer/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "index.html",
      ".env"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "assets/icon.ico"
    }
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  }
}
